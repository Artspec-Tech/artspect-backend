{
  "name": "artspec-backend",
  "version": "1.0.0",
  "description": "Backend system for artspec",
  "main": "server.js",
  "repository": "https://github.com/Artspec-Tech/artspec-backend.git",
  "author": "Nonthapat Kaewamporn <tontan2545@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "husky": "^7.0.4",
    "lint-staged": ">=10",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^4.6.3"
  },
  "scripts": {
    "start": "ts-node ./src/server.ts",
    "dev": "nodemon ./src/server.ts",
    "lint": "prettier --config .prettierrc 'src/**/*.ts' --check",
    "lint:fix": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "postinstall": "husky install",
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "envalid": "^7.3.0",
    "express": "^4.17.3",
    "ts-node": "^10.7.0",
    "typedi": "^0.10.0",
    "winston": "^3.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts}": "prettier --config .prettierrc --write"
  }
}
